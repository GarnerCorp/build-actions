{
  "problemMatcher": [
    {
      "owner": "sbt-publishing",
      "severity": "error",
      "pattern": [
        {
          "regexp" : "(java\\.io\\.IOException): PUT operation failed because the (destination file exists and overwriting is disabled)",
          "code": 1,
          "message": 2
        }
      ]
    },
    {
      "owner": "scala-test",
      "severity": "error",
      "pattern": [
        {
          "regexp" : "- (.*) \\*\\*\\* FAILED \\*\\*\\*",
          "message": 1
        },
        {
          "regexp" : "\\((\\w+\\.scala):(\\d+)\\)$",
          "file": 1,
          "line": 2,
          "loop": true
        }
      ]
    }
  ]
}
